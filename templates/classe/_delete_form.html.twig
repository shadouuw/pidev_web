<form method="post" action="{{ path('classe_delete', {'id': classe.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ classe.id) }}">
    <button  type="submit" class="btn btn-gradient-danger btn-rounded btn-fw">Delete</button>
</form>
